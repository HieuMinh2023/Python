number = 1
oringinal_number = 2
sequence = ''

for n in range(1,11,+1):
    for n in range(1,11,+1):
        sequence = sequence + ' ' + str(number)
        number = int(number) + 10
    
    print(sequence)
    print()
    sequence = ''
    number = oringinal_number
    oringinal_number = oringinal_number + 1
